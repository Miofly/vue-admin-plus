{
  "scripts": {
    "prepare": "husky install",
    "bootstrap": "pnpm install",
    "clean": "pnpm /^clean:/",
    "clean:dist": "rimraf dist",
    "clean:node_modules": "rimraf node_modules",
    "dev": "cd admin && vite",
    "build": "cd admin && vite build",
    "log": "conventional-changelog -p angular -i CHANGELOG.md -s"
  },
	"dependencies": {
		"unplugin-vue-macros": "^2.0.0"
	},
  "devDependencies": {
    "@types/node": "18.8.0",
    "conventional-changelog-cli": "^2.2.2",
    "husky": "8.0.3",
    "rimraf": "^4.4.1"
  },
  "engines": {
    "node": ">=16"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "vue",
        "postcss",
        "typescript",
        "vite",
        "terser",
        "eslint",
        "stylelint",
        "rollup",
        "@types/node",
        "@types/markdown-it",
        "pinia"
      ]
    }
  }
}
